{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-old-space-size=3000 && next dev",
    "build": "next build --no-lint",
    "build:analyze": "set ANALYZE=true && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "generate-apk": "powershell -ExecutionPolicy Bypass -File generate-apk.ps1",
    "generate-apk-bash": "bash generate-apk.sh",
    "convert-icons": "powershell -ExecutionPolicy Bypass -File convert-icons.ps1",
    "pwa-audit": "lighthouse http://localhost:3000 --only-categories=pwa --chrome-flags=\"--headless\"",
    "install-bubblewrap": "npm install -g @bubblewrap/cli",
    "pwa-test": "npm run build && npm run start & npm run pwa-audit"
  },
